{
  "name": "synthetix-subgraph",
  "license": "MIT",
  "version": "1.2.1",
  "author": "Synthetix",
  "main": "index.js",
  "browser": "index.min.js",
  "bin": {
    "snxData": "bin.js"
  },
  "scripts": {
    "lint": "eslint .",
    "yaml:chainlink": "node ./create-yaml-file create-yaml --subgraph chainlink --env prod --universal-test-block null | mustache - mustache/yaml.mustache > subgraphs/synthetix-chainlink.yaml",
    "yaml:exchanger": "node ./create-yaml-file create-yaml --subgraph exchanger --env prod --universal-test-block null | mustache - mustache/yaml.mustache > subgraphs/synthetix-exchanger.yaml",

    "codegen": "graph codegen",
    "codegen:chainlink": "npm run yaml:chainlink && npm run codegen -- subgraphs/synthetix-chainlink.yaml",
    "codegen:exchanger": " npm run codegen -- subgraphs/synthetix-exchanger.yaml",

    "build": "graph build",
    "build:chainlink": "graph build subgraphs/synthetix-chainlink.yaml",
    "build:exchanger": "graph build subgraphs/synthetix-exchanger.yaml",


    "deploy": "graph deploy --node  https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ allenhyfay/sugmrm",
    "deploybsc": "graph deploy --node http://119.8.19.36:8020/ --ipfs http://119.8.19.36:5001 synthetixio-team/synthetix",
    "deploy:chainlink": "npm run deploy -- subgraphs/synthetix-chainlink.yaml",
    "deploy:exchanger": "npm run deploybsc -- subgraphs/synthetix-exchanger.yaml",

    "create-local": "graph create --node http://119.8.19.36:8020/ synthetixio-team/synthetix",
    "remove-local": "graph remove --node http://42.192.186.62:8020/ synthetixio-team/synthetix",
    "deploy-local": "graph deploy --node http://192.168.6.249:8020/ --ipfs http://192.168.6.249:5001 synthetixio-team/synthetix"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.25.1",
    "@graphql-tools/merge": "^8.2.1",
    "commander": "2.20.0",
    "mustache": "4.0.1",
    "chalk": "4.1.2",
    "commander": "8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "synthetix": "2.56.0-alpha"
  },
  "devDependencies": {
    "@graphprotocol/graph-ts": "^0.24.1",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "minify": "4.1.3",
    "prettier": "1.17.1",
    "typescript": "4.4.3"
  }
}
